{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "run",
			"problemMatcher": [
				"$rustc"
			],
			"args": [
				"--example", "serial",
				"--features=std,serial"
			],
			"label": "rust: cargo run --example serial --features=std,serial",
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	]
}